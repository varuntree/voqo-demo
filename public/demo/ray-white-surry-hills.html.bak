    <script>
        // Agency context for API calls
        const agencyContext = {
            id: "ray-white-surry-hills",
            name: "Ray White Surry Hills",
            website: "https://raywhitesurryhills.com.au/",
            phone: "+61 2 9004 8800",
            email: "peter.natoli@raywhite.com",
            address: "Suite 9-11, 410 Crown Street, Surry Hills NSW 2010",
            metrics: {
                teamSize: 50,
                listingCount: 44,
                hasPropertyManagement: true,
                principalName: "Peter Natoli"
            },
            painScore: 70,
            painReasons: [
                "44 active listings generating high call volume",
                "No after-hours contact solution",
                "No chat widget for instant inquiries",
                "Large PM portfolio needs 24/7 coverage"
            ]
        };

        // ROI Calculator
        function calculateROI() {
            const missedCalls = parseInt(document.getElementById('missedCalls').value) || 0;
            const avgCommission = parseInt(document.getElementById('avgCommission').value) || 0;
            const conversionRate = parseFloat(document.getElementById('conversionRate').value) || 0;

            const weeksPerYear = 52;
            const callsPerYear = missedCalls * weeksPerYear;
            const salesPerYear = (callsPerYear * (conversionRate / 100));
            const annualRevenue = salesPerYear * avgCommission;

            document.getElementById('callsCaptured').textContent = callsPerYear.toLocaleString();
            document.getElementById('additionalSales').textContent = salesPerYear.toFixed(1);
            document.getElementById('annualSavings').textContent = '$' + annualRevenue.toLocaleString();
        }

        // Modal functions
        function openCallModal() {
            document.getElementById('callModal').classList.add('active');
        }

        function closeCallModal() {
            document.getElementById('callModal').classList.remove('active');
        }

        // Register call and initiate demo
        async function initiateCall() {
            const button = document.querySelector('#callModal .btn-primary');
            button.classList.add('loading');

            try {
                // Register call context with the API
                const response = await fetch('/api/register-call', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify({
                        agencyId: agencyContext.id,
                        agencyName: agencyContext.name,
                        context: {
                            source: 'demo-page',
                            timestamp: new Date().toISOString(),
                            agency: agencyContext,
                            userAgent: navigator.userAgent,
                            referrer: document.referrer
                        }
                    })
                });

                const data = await response.json();

                if (data.success && data.phoneNumber) {
                    // Redirect to phone call
                    window.location.href = `tel:${data.phoneNumber}`;
                } else {
                    // Fallback to default demo number
                    window.location.href = 'tel:+61483943567';
                }
            } catch (error) {
                console.error('Error registering call:', error);
                // Fallback to default demo number on error
                window.location.href = 'tel:+61483943567';
            } finally {
                button.classList.remove('loading');
                closeCallModal();
            }
        }

        // Close modal on overlay click
        document.getElementById('callModal').addEventListener('click', function(e) {
            if (e.target === this) {
                closeCallModal();
            }
        });

        // Close modal on escape key
        document.addEventListener('keydown', function(e) {
            if (e.key === 'Escape') {
                closeCallModal();
            }
        });

        // Initialize ROI calculator
        calculateROI();

        // Track page view
        console.log('Demo page loaded for:', agencyContext.name);
    </script>
